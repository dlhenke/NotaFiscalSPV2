{
    "name": "dlhenke/notafiscalspv2",
    "description": "Integração com o WebService da Prefeitura de São Paulo para emissão de NFS e NTFS utilizando a API da nota do milhão Versão 2 (2026 Reforma Tributária)",
    "type": "library",
    "license": "MIT",
    "require": {
        "robrichards/xmlseclibs": "^3.0",
        "greenter/xmldsig": "^5.0",
        "spatie/array-to-xml": "^2.8",
        "ext-dom": "*",
        "ext-soap": "*",
        "vlucas/phpdotenv": "^5.6"
    },
    "suggest": {
        "ext-mbstring": "*"
    },
    "keywords": [
        "NFS",
        "NFTS",
        "Emissão Nota Fiscal",
        "Nota Fiscal",
        "Nota do Milhão",
        "Prefeitura São Paulo",
        "Nota Fiscal Tomador de Serviços",
        "Nota Fiscal Prefeitura de Sao Paulo Versao 2 (2026 Reforma Tributaria)"
    ],
    "authors": [
        {
            "name": "Durval Henke",
            "email": "durvalhenke@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NotaFiscalSP\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
